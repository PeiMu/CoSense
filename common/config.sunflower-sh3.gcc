AR			= sh-coff-ar
OBJECTEXTENSION		= o
LD			= sh-coff-ld
CC			= sh-coff-gcc
CCFLAGS			= "-nostdlib -fno-builtin -std=gnu99"
RANLIB			= sh-coff-ranlib
HOSTNAME		= localhost#`hostname`
HTMLBASEPATH		= /Library/WebServer/Documents/tmp
PLATFORM_DFLAGS		+= -DkNoisyBasePath="\"$(HTMLBASEPATH)/\"" -DkNoisyDotCommand="\"/opt/local/bin/dot\"" -DkNoisyLogPath="\"http://$(HOSTNAME)/tmp/salsvm-logo.png\"" -DkOrganizationLogoPath="\"http://$(HOSTNAME)/tmp/orange-logo-full.png\"" -DkNoisyCgiFileUrlBase=\""http://$(HOSTNAME)/tmp/\"" -DkNoisyCgiExecutableUrl="\"http://$(HOSTNAME)/cgi-bin/crayoncgi-darwin\"" -DkNewtonCgiExecutableUrl="\"http://$(HOSTNAME)/cgi-bin/newtoncgi-darwin\""
PLATFORM_OPTFLAGS	= -O3
PLATFORM_DBGFLAGS	+= -g
PLATFORM_CFLAGS		+= -DNOISY_OS_MACOSX
