#
#	Description:	Empty invariant for auto-generating synthetic invariants
#			for a gas-pressure sensor.
#
#	Parameters
#
#		E		: Emission factor
#		Emitter_area		: Emitter area
#		T_E		: Emitter temperature
#		mp_l		: Absorption length - measurement path
#		dc_l   	: Absorption length -detection chamber
#       T_env    	: Environmental Temperature
#		P_atm	: Static Pressure
#		P_signal: Measures Pressure change
#		t       : Time elapsed
#		N_0     : Molecular density
#		f 		: Frequency 
#		k 		: Exitation Probability
#		e 		: mean energy of an absorbed photon
#		t_d 	: Decay constant-lifetime excited state (spontaneous relaxation)
#		t_c 	: Decay constant-collisional relaxation
#		t_th 	: Decay constant- Thermal heat losses
#		V 		: Volume of detection chamber

include "NewtonBaseSignals.nt"

E_dim : signal =
{
	derivation = 1;
}

N_0_dim : signal =
{
	derivation = 1/volume;
}

k_dim : signal =
{
	derivation = 1;
}

ExplosionRadiusForPiGroups: invariant(	E:	E_dim,
					Emitter_area:	area,
					T_E:	temperature,
					mp_l:	distance,
                    dc_l: distance,
					T_env:	temperature,
                    P_atm: pressure,
                    P_signal: pressure,
                    t : time,
                    N_0 : N_0_dim,
                    f : frequency,
                    k : k_dim,
                    e : work,
                    t_d : time,
                    t_c : time, 
                    t_th : time, 
                    V : volume 
                    ) =
{
}
