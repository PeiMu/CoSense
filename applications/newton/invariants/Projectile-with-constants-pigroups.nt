#
#	Description:	Empty invariant for auto-generating synthetic invariants
#			for a projectile.
#
#	Parameters
#
#		h :
#		v :
#		m :
#		g :
#

include "NewtonBaseSignals.nt"

v0	: constant = 0 (meter*second**-1);

ProjectileForPiGroups: invariant(	h_p: distance,
					v_p: speed,
					m_p: mass) =
{
	h_p ~ {v_p, m_p, v0, kNewtonUnithave_AccelerationDueToGravity}
}
