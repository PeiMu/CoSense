#
#	See https://github.com/phillipstanleymarbell/Noisy-lang-compiler/issues/125
#

distance : signal =
{
        name = "meter" English;
        symbol = m;
        derivation = none;
}

mass : signal =
{
        name = "kilogram" English;
        symbol = kg;
        derivation = none;
}

poisson : constant = 0.272;
rho : constant = 1040 * kilogram * meter ** -3;
E : constant = 1.2e9 * Newton / meter ** -2;

null : invariant(null: mass) =
{
	null ~ 0
}
