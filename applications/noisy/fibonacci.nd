Fibonacci : progtype (indexType: type, valueType: type)
{
	fibonacci : namegen (index: indexType) -> (result: valueType);
	fibonacci : probdef (index: indexType) -> (result: valueType);
}

fibonacci : probdef (index: valueType) -> (result: valueType) =>
{
	output == (((((1 + (5 ** (1/2)))/2) ** index) / (5 ** (1/2))) + (1/2)) - ((((((1. + (5 ** (1/2)))/2) ** index) / (5 ** (1/2))) + (1/2)) % 1)
}
