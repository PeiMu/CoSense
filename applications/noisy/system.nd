#
#	The `System` progtype implements the standard library.
#
System : progtype
{
	print	: function (args: list of string) -> (utf8Bytes: int64);
	print	: probdef (args: list of string) -> (utf8Bytes: int64);
}

print	: probdef (args: list of string) -> (utf8Bytes: int64) =>
{
	utf8Bytes == 
	(
		sum for (strings in args) of 
		(
			length (strings)
		)
	)
}
