AR			= /usr/bin/ar
RANLIB			= /usr/bin/ranlib
HOSTNAME		= localhost#`hostname`
HTMLBASEPATH		= /var/www/html/tmp/
PLATFORM_DFLAGS		+= -DkPhyscomplabLogoPath="\"http://$(HOSTNAME)/tmp/orange-logo-full.png\"" -Dk$(SYSNAME)BasePath="\"$(HTMLBASEPATH)\"" -Dk$(SYSNAME)DotCommand="\"/usr/bin/dot\"" -Dk$(SYSNAME)LogoPath="\"http://$(HOSTNAME)/tmp/$(SYSNAME)-logo.png\"" -Dk$(SYSNAME)CgiFileUrlBase=\""http://$(HOSTNAME)/tmp/\"" -Dk$(SYSNAME)CgiExecutableUrl="\"http://$(HOSTNAME)/cgi-bin/$(SYSNAMELOWER)cgi-linux\"" -Dk$(SYSNAME)CgiExecutableUrl="\"http://$(HOSTNAME)/cgi-bin/$(SYSNAMELOWER)cgi-linux\""
PLATFORM_OPTFLAGS	= -O0
PLATFORM_DBGFLAGS	+= -g
PLATFORM_CFLAGS		+= -D$(SYSNAME)OsLinux
CGI_BIN			= /usr/lib/cgi-bin/
LINTFLAGS		= --analyze
